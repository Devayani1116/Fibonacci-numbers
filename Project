n=int(input("How many terms do you need in the fibonacci sequence? "))
x=0
y=1
count=2
if n<=0:
    print("enter a positive value")
elif n==1:
    print("the fibonacci sequence with",n,"terms",x)
else:
    print(x)
    print(y)
    while count<n:
        temp=x+y
        print(temp)
        x=y
        y=temp
        count+=1
